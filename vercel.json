{
	"version": 2,
	"builds": [
	  {
		"src": "app.js",
		"use": "@now/node"
	  }
	],
	"routes": [
	  {
		"src": "/(.*)",
		"dest": "app.js"
	  }
	],
	"name": "rest-api",
	"description": "mi primera rest api con express",
	"main": "app.js",
	"type": "module",
	"scripts": {
	"start": "node app.js",
	"main": "node --watch app.js",
	"test": "echo \"Error: no test specified\" && exit 1"
	},
	"author": "Alexwithstars",
	"license": "ISC",
	"dependencies": {
	"express": "4.18.2",
	"mime-types": "^2.1.35",
	"mysql2": "3.6.2",
	"zod": "3.22.4"
}
}